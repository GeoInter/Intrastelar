<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thb.fbi.controller.MemoryController">
   <children>
      <TextField fx:id="startAddressLabel" promptText="Interval Start" />
      <TextField fx:id="endAddressLabel" promptText="Interval End" />
      <Button mnemonicParsing="false" text="Temp">
         <VBox.margin>
            <Insets bottom="8.0" top="8.0" />
         </VBox.margin>
      </Button>
      <TableView fx:id="memoryTable">
        <columns>
          <TableColumn fx:id="addressColumn" editable="false" prefWidth="75.0" sortable="false" text="Address" />
          <TableColumn fx:id="contentColumn" editable="false" prefWidth="75.0" sortable="false" text="Content" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
